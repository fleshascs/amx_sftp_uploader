{
  "name": "amx_sftp_uploader",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "node build/src/server.js",
    "prebuild": "npm run lint",
    "build:typescript": "tsc -p tsconfig.release.json",
    "build:linux": "npx pkg -t linux -o dist/fastupload ./build/src/server.js",
    "build:windows": "npx pkg -t windows -o dist/fastupload ./build/src/server.js",
    "build": "npm run build:typescript && npm run build:windows",
    "build:release": "npm run build && cp example.config.yml ./dist/config.yml",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "npx jest --coverage",
    "test:watch": "npx jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/fancy-log": "^2.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.7.10",
    "@types/ramda": "^0.28.15",
    "@types/ssh2-sftp-client": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "~4.25.0",
    "eslint": "~7.27.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.3.6",
    "jest": "^27.5.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^27.1.4",
    "ts-node": "^8.10.2",
    "tsutils": "~3.21.0",
    "typescript": "~4.3.2"
  },
  "dependencies": {
    "cli-color": "^2.0.3",
    "fancy-log": "^1.3.3",
    "node-watch": "^0.7.1",
    "ramda": "^0.28.0",
    "ssh2-sftp-client": "^9.0.2",
    "yaml": "^2.1.1"
  }
}
